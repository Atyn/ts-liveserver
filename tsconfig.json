{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "CommonJS",
		"target": "ES2020",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"tsBuildInfoFile": ".cache/tsc/tsbuildinfo",
		"composite": true,
		"incremental": true,
		"jsx": "react",
		"lib": ["dom", "ESNext"]
	},
	"references": [
		{
			"path": "packages/build"
		},
		{
			"path": "packages/express-middleware"
		},
		{
			"path": "packages/server"
		},
		{
			"path": "packages/test-environment"
		},
		{
			"path": "packages/ts-transpiler"
		}
	]
}
